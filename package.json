{
  "name": "writinator",
  "description": "An elegant writing tool that combines Plottr and simple writing",
  "repository": "https://github.com/cameronsutter/writinator_electron.git",
  "private": true,
  "readme": "Copyright 2016 C. Louis S. (Cameron Sutter)",
  "version": "0.0.1",
  "author": {
    "name": "C. Louis S.",
    "email": "cameronsutter0@gmail.com",
    "url": "https://medium.com/@camsutter"
  },
  "main": "bin/main.js",
  "scripts": {
    "start": "NODE_ENV=dev electron .",
    "compile": "node_modules/.bin/webpack",
    "watch": "echo 'do you have fsevents?' && NODE_ENV=dev node_modules/.bin/webpack -w",
    "createDevLicense": "NODE_ENV=license electron .",
    "changelog": "changelog cameronsutter/plottr_electron all -m > changelog.md",
    "pack": "NODE_ENV=production node_modules/.bin/webpack 1> /dev/null",
    "pack:windows": "node_modules\\.bin\\webpack",
    "dist": "node build/initEnv.js && npm run pack && build",
    "dist:windows": "node build/initEnv.js && npm run pack:windows && build",
    "dist:all": "npm run pack && build -mw"
  },
  "dependencies": {
    "docx": "^2.1.1",
    "electron-json-storage": "^2.0.0",
    "electron-log": "^2.2.9",
    "lodash": "^4.17.4",
    "mixpanel-browser": "^2.9.14",
    "pretty-date": "^0.2.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-overlays": "^0.8.3",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "request": "^2.74.0",
    "rollbar": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "changelog": "^1.0.7",
    "css-loader": "^0.22.0",
    "dotenv": "^4.0.0",
    "electron-builder": "^19.24.1",
    "electron-devtools-installer": "^2.0.0",
    "electron-packager": "^7.7.0",
    "electron": "^1.3.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "build": {}
}
